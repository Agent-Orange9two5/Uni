#include <stdio.h>
#include <stdlib.h>
int main()
{

    int n,i,k;
    scanf("%d",&n);
   // scanf("%d",&k);
    int a[n][n];
    int p=n*n;
    for(i=0;i<p;i++)  scanf("%d",*a+i);

    //afisarea liniei k
    printf("\n");
    printf("Linia k: ");
    for(i=0;i<n;i++) { printf("%d ",*(*a+((k-1)*n)+i));
    if((i+1)%n==0) printf("\n");  }

    //afisarea liniei k
    printf("\n");
    printf("Coloana k: ");
    for(i=0;i<n;i++) { printf("%d ",*(*a+k+i*n));
    if((i+1)%n==0) printf("\n");  }

    //afisarea diagonalei principale
    printf("\n");
    printf("Diagonala principala:");
    for(i=0;i<n;i++)  printf("%d ",*(*a+i*(n+1)));

  
     //afisarea diagonalei secundare
    printf("\n");
    printf("Diagonala secundara:");
    for(i=1;i<=n;i++)  printf("%d ",*(*a+i*(n-1)));

    printf("\n");
    printf("\n");

 /*   int i,j,n;         (citire cu afisare metoda semi-veche)
    scanf("%d",&n);
    int a[n][n];
    for(i=0;i<n*n;i++)
  //  for(j=0;j<n;j++)
        scanf("%d",*a+i);

    for(i=0;i<n;i++)
    {
    for(j=0;j<n;j++)
        printf("%d ",a[i][j]);
        printf("\n");
    }
    printf("\n"); */


    //afisarea elementului de la intersectia diagonalelor
    printf("%d ",*(*(a+n/2)+n/2));




    return 0;
}
